{
  "extends": ["react-app", "import"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn",
    "no-const-assign": "error",
    "no-underscore-dangle": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "useTabs": true,
        "trailingComma": "all"
      }
    ]
  }
}
